{% extends 'blog/layout.html.twig' %}

{% block content %}
    
    {{ paginator(paginator, 'website.blog.index') }}
    
    {% for post in posts %}
        <div class="">
            <p>{{ post.published|date('d/m/Y, H:i') }}</p>
            <h3>
                {{ post.title }}
            </h3>
        </div>
    {% endfor %}
    
{% endblock %}