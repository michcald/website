{% spaceless %}
{% if paginator.page.total > 1 %}
    <div class="text-center">
        <ul class="pagination">
            <li class="disabled">
                <a href="">
                    Results <strong>{{ paginator.results }}</strong>
                </a>
            </li>

            {% if paginator.page.current > 1 %}
            <li>
                <a href="{{ url(routeId, {
                        page: 1
                    }|merge(routeParams)) }}">First</a>
            </li>
            {% else %}
            <li class="disabled">
                <a href="#">First</a>
            </li>
            {% endif %}

            <!-- Start Go back //-->

            {% if paginator.page.current > 1 %}
            <li>
                <a href="{{ url(routeId, {
                        page: paginator.page.current - 1
                    }|merge(routeParams)) }}">Prev</a>
            </li>
            {% else %}
            <li class="disabled">
                <a href="#">Prev</a>
            </li>
            {% endif %}

            <!-- End Go back //-->

            <!-- Start Pages list //-->

            {% for i in range(1, paginator.page.total) %}
                <li{% if i == paginator.page.current %} class="active"{% endif %}>
                    <a href="{{ url(routeId, {
                        page: i
                    }|merge(routeParams)) }}">
                        {{ i }}
                    </a>
                </li>
            {% endfor %}

            <!-- End Pages list //-->

            <!-- Start Go next //-->

            {% if paginator.page.current < paginator.page.total %}
            <li>
                <a href="{{ url(routeId, {
                    page: paginator.page.current+1
                }|merge(routeParams)) }}">Next</a>
            </li>
            {% else %}
            <li class="disabled">
                <a href="#">Next</a>
            </li>
            {% endif %}

            <!-- End Go next //-->

            {% if paginator.page.current < paginator.page.total %}
            <li>
                <a href="{{ url(routeId, {
                    page: paginator.page.total
                }|merge(routeParams)) }}">Last</a>
            </li>
            {% else %}
            <li class="disabled">
                <a href="#">Last</a>
            </li>
            {% endif %}

            <li class="disabled">
                <a href="">
                    Page
                    <strong>{{ paginator.page.current }}</strong>
                    of
                    <strong>{{ paginator.page.total }}</strong>
                </a>
            </li>
        </ul>
    </div>
{% endif %}
{% endspaceless %}